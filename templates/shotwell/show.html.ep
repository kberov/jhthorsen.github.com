% layout 'default';
% title $name;
<h1><%= $name %></h1>
<div id="navbar">
  <ul class="nav nav-tabs">
    <li><%= link_to 'Raw format', $raw %></li>
    <li><%= link_to 'Download', "$raw?download=1" %></li>
  </ul>
</div>
<div class="single-thumbnail thumbnail span8">
  %= link_to $raw, begin
    %= image "$raw?inline=1024", alt => $name;
  % end
  <div class="caption">
    <h5><%= $name %></h5>
  </div>
</div>
