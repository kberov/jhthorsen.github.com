% layout 'default';
% title $name;
<h1 class="page-header"><%= $name %></h1>
%= include 'shotwell/navbar'
<ul class="gallery" id="gallery">
  % for my $photo (@$photos) {
  <li id="<%= $photo->{id} %>">
    %= link_to $photo->{url}, begin
      %= image '/image/not-loaded.png', 'data-src' => $photo->{thumb}, alt => $photo->{name}, title => $photo->{name}
    % end
  </li>
  % }
</ul>
