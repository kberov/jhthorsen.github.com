% layout 'default';
% title 'Events';
<h1 class="page-header">Events / <%= link_to 'Tags', 'shotwell/tags' %></h1>
<ul class="files">
  % unless(@$events) {
  <li>No events.</li>
  % }
  % for my $event (@$events) {
  <li id="<%= $event->{id} %>">
    %= link_to $event->{name}, $event->{url}
    <small><%= localtime $event->{time_created} %></small>
  </li>
  % }
</ul>
