% layout 'default';
% title 'Events';
<h1>Events / <%= link_to 'Tags', 'shotwell_tags' %></h1>
<ul class="files">
  % unless(@$files) {
  <li>No events.</li>
  % }
  % for my $f (@$files) {
  <li id="<%= $f->{id} %>">
    %= link_to $f->{basename}, $f->{url}
    <small><%= $f->{date} %></small>
  </li>
  % }
</ul>
